{
  "name": "numerical-comment",
  "displayName": "numerical comment",
  "description": "structured commands",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "numerical-comment.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "prettier": "prettier --write ./*"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "vsce": "^2.15.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "icon": "icon.png",
  "license": "MIT",
  "author": {
    "name": "wuchuheng",
    "email": "root@wuchuheng.com"
  },
  "repository": {
    "url": "https://github.com/wuchuheng/vscode-comment-hierarchy",
    "type": "git"
  },
  "publisher": "numericalcomment"
}